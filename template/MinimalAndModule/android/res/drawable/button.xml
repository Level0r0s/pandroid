<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/BackgroundPressColor02" />
            <stroke
                android:width="1dp"
                android:color="@color/LinePressColor02" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
           <solid
              android:color="@color/BackgroundColor02" />

            <stroke
                android:width="1dp"
                android:color="@color/LineColor02" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="10dp"
                android:top="5dp"
                android:right="10dp"
                android:bottom="5dp" />
        </shape>
    </item>
</selector>
